remove mod5 = Mode_switch

! caps -> ctrl, win -> Mode_switch
keycode 66 = Control_L Caps_Lock Control_L Caps_Lock
keycode 133 = Mode_switch

! henkan -> Mode
keycode 100 = Mode_switch

! 203 (no exists) -> ctrl
keycode 203 = Control_L

! menu -> ctrl
